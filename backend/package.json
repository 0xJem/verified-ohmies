{
  "name": "slash-create-vercel-template",
  "version": "1.0.0",
  "description": "A template for slash-create",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "vercel-build": "yarn run typechain --target ethers-v5 --out-dir typechain abi/*.json abi/**/*.json && tsc && node dist/scripts/sync",
    "build": "yarn run typechain --target ethers-v5 --out-dir typechain abi/*.json abi/**/*.json && tsc",
    "lint": "npx eslint --ext .ts .",
    "lint:fix": "npx eslint --ext .ts . --fix",
    "gen": "graphql-codegen --config codegen.yml",
    "typechain:build": "yarn run typechain --target ethers-v5 --out-dir typechain abi/*.json abi/**/*.json"
  },
  "dependencies": {
    "ethers": "^5.0.17",
    "etherscan-api": "^10.0.5",
    "express": "^4.17.1",
    "graphql": "^16.1.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "2.6.1",
    "slash-create": "^4.1.1",
    "typechain": "^7.0.0",
    "typescript": "^4.4.2",
    "uuidv4": "6.2.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@typechain/ethers-v5": "^9.0.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vercel/node": "^1.12.1",
    "discord.js": "12.5.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1"
  }
}
